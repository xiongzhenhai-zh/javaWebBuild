	#if(panel.template=="add")
	#for(Relation relation:panel.store.model.relations)
	#if((relation.type=="ManyToOne"||relation.type=="OneToOne")&&relation.name!="parent")
	lc.ns.${page.name}.init${relation.relation.model.name}Combobox();
	#end
	#end
	#end
	#end
}
</script>