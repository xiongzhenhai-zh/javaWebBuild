lc.ns.${page.name}.onPageChange = function(){
	$("#panel_list_${page.name}").datagrid("reload");
	$("#panel_tree_${page.name}").tree("reload");
}

lc.currPageInit = function(){
	lc.ns.${page.name}.loadList();
	lc.ns.${page.name}.tree();
